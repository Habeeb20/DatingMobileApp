{
  "expo": {
    "name": "letsmeet",
    "slug": "letsmeet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myLogo.jpg",
    "scheme": "letsmeet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image":  "./assets/images/myLogo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage":  "./assets/images/myLogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
